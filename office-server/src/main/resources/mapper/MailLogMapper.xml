<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.timberkito.server.mapper.MailLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.timberkito.server.pojo.MailLog">
        <id column="msgId" property="msgId"/>
        <result column="eid" property="eid"/>
        <result column="status" property="status"/>
        <result column="routeKey" property="routeKey"/>
        <result column="exchange" property="exchange"/>
        <result column="count" property="count"/>
        <result column="tryTime" property="tryTime"/>
        <result column="createTime" property="createTime"/>
        <result column="updateTime" property="updateTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        msgId, eid, status, routeKey, exchange, count, tryTime, createTime, updateTime
    </sql>

</mapper>
